		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-plugin-alias": "^0.2.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "5.0.4"
	},
	"dependencies": {
		"chrono-node": "^2.8.0",
